    <function>
        <name>appConfig</name>
        <function-class>md.euroreparatii.ui.el.functions.Utils</function-class>
        <function-signature>md.euroreparatii.config.AppConfig appConfig(javax.servlet.jsp.PageContext)</function-signature>
    </function>
