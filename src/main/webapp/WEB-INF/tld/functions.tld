        version="2.0">

    <tlib-version>3.0</tlib-version>
    <short-name>functions</short-name>
    <uri>/WEB-INF/tld/erfn.tld</uri>

    <function>
        <name>curentLocaleCode</name>
        <function-class>md.euroreparatii.ui.el.functions.Utils</function-class>
        <function-signature>java.lang.String currentLocaleCode(javax.servlet.jsp.PageContext)</function-signature>
